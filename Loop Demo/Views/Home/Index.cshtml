@{
    ViewBag.Title = "My Favorite Fruit";

    string[] members = (string[])ViewData["passedArray"];

}


<h3>  @ViewBag.Title  </h3>
<div class="row">


    <div class="col-md-4">
        <br />
        <br />


        @foreach (var item in members)
            {
            <p>@item</p>
        }
    </div>
    <div class="col-md-4">
        <br />
        <br />
        @for (var i = 1; i < 11; i++)
            {<p>Line @i</p>}

    </div>
    <div class="col-md-4">

        <div>


        </div>
    </div>


</div>